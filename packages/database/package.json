{
	"name": "@trpc-poc/database",
	"version": "0.1.0",
	"type": "module",
	"main": "./index.ts",
	"types": "./index.ts",
	"scripts": {
		"esm": "rm -rf ./prisma/generated/prisma-client-es && rollup -c rollup.config.js && cp ./prisma/generated/prisma-client/index.d.ts ./prisma/generated/prisma-client-es/index.d.ts",
		"db:generate": "pnpm run db:generate:clean && pnpm run esm",
		"db:generate:clean": "prisma generate",
		"db:push": "prisma db push --skip-generate",
		"postinstall": "prisma generate"
	},
	"dependencies": {
		"@prisma/client": "^4.8.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^23.0.2",
		"prisma": "^4.8.0",
		"rollup": "^2.79.1",
		"rollup-plugin-dts": "^5.1.1",
		"tsx": "^3.12.1",
		"typescript": "^4.6.4"
	}
}
