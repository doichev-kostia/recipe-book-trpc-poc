{
	"name": "trpc-poc",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"watch": "turbo run watch --parallel",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"prettier": "^2.7.1",
		"turbo": "^1.6.2",
		"typescript": "^4.6.4"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"packageManager": "pnpm@7.0.0"
}
