{
	"name": "@trpc-poc/api",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"scripts": {
		"start": "cross-env NODE_PATH=./build node -r source-map-support/register build/server.js",
		"dev": "cross-env NODE_ENV=development NODE_PATH=./build nodemon -r source-map-support/register ./build/server.js",
		"build": "tsc",
		"build:clean": "pnpm i && pnpm clean:build && pnpm build",
		"clean:build": "pnpm rimraf build",
		"clean:modules": "pnpm rimraf node_modules",
		"watch": "tsc -w",
		"test": "cross-env NODE_PATH=./build vitest --config ./vitest.config.ts --dir ./build",
		"coverage": "pnpm test run --coverage",
		"test:ui": "pnpm run test --ui",
		"migration:revert": "pnpm mikro-orm migration:down",
		"migration:migrate": "pnpm mikro-orm migration:create && pnpm mikro-orm migration:up",
		"load-fixtures:test": "cross-env NODE_ENV=test NODE_PATH=./build node build/scripts/load-fixtures.js",
		"load-fixtures:app": "cross-env NODE_ENV=development NODE_PATH=./build node build/scripts/load-fixtures.js"
	},
	"main": "build/server.js",
	"types": "build/server.d.ts",
	"module": "build/server.js",
	"exports": {
		".": {
			"types": "./build/server.d.ts",
			"import": "./build/server.js",
			"require": "./build/server.js"
		},
		"./package.json": "./package.json"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@jest/globals": "^29.3.1",
		"@jest/types": "^29.3.1",
		"@types/bcryptjs": "^2.4.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.14",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^29.2.2",
		"@types/node": "16.11.7",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^8.3.4",
		"bcryptjs": "^2.4.3",
		"dotenv": "^16.0.3",
		"jest": "^29.3.1",
		"jest-junit": "^14.0.1",
		"mockdate": "^3.0.5",
		"nodemon": "^2.0.20",
		"rimraf": "^3.0.2",
		"supertest": "^6.3.1",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.6.4"
	},
	"dependencies": {
		"@mikro-orm/cli": "^5.5.1",
		"@mikro-orm/core": "^5.5.1",
		"@mikro-orm/migrations": "^5.5.1",
		"@mikro-orm/postgresql": "^5.5.1",
		"@panenco/papi": "^1.1.1",
		"@trpc-poc/contracts": "workspace:*",
		"@trpc/server": "10.0.0-proxy-beta.26",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"date-fns": "^2.29.3",
		"express": "^4.18.2",
		"fs-extra": "^10.1.0",
		"hash-files": "^1.1.1",
		"reflect-metadata": "^0.1.13",
		"source-map-support": "^0.5.21",
		"uuid": "^9.0.0",
		"zod": "^3.19.1"
	},
	"mikro-orm": {
		"useTsNode": true,
		"configPaths": [
			"./src/orm.config.ts"
		]
	}
}
