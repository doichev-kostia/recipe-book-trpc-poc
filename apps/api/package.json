{
	"name": "@trpc-poc/api",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"scripts": {
		"start": "cross-env NODE_PATH=./build node -r source-map-support/register build/server.js",
		"dev": "cross-env NODE_ENV=development NODE_PATH=./build nodemon -r source-map-support/register ./build/server.js",
		"build": "tsc",
		"build:clean": "pnpm i && pnpm clean:build && pnpm build",
		"clean:build": "pnpm rimraf build",
		"clean:modules": "pnpm rimraf node_modules",
		"watch": "tsc -w"
	},
	"main": "build/server.js",
	"types": "build/server.d.ts",
	"module": "build/server.js",
	"exports": {
		".": {
			"types": "./build/server.d.ts",
			"import": "./build/server.js",
			"require": "./build/server.js"
		},
		"./package.json": "./package.json"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.14",
		"@types/node": "16.11.7",
		"dotenv": "^16.0.3",
		"nodemon": "^2.0.20",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.6.4"
	},
	"dependencies": {
		"@trpc-poc/contracts": "workspace:*",
		"@trpc/server": "10.0.0-proxy-beta.26",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"express": "^4.18.2",
		"source-map-support": "^0.5.21",
		"zod": "^3.19.1"
	}
}
